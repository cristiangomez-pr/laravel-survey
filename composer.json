{
  "name": "matt-daneshvar/laravel-survey",
  "description": "Create surveys inside your Laravel app",
  "keywords": [
    "laravel",
    "survey"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Daneshvar",
      "email": "matt.daneshvar@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/database": "^9.0",
    "maatwebsite/excel": "^3.1",
    "livewire/livewire": "^2.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0",
    "illuminate/http": ">=6",
    "mockery/mockery": "^1.4",
    "laravel/legacy-factories": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "MattDaneshvar\\Survey\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MattDaneshvar\\Survey\\Tests\\": "tests/"
    },
    "files": [
      "tests/utilities/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MattDaneshvar\\Survey\\SurveyServiceProvider"
      ]
    }
  }
}
